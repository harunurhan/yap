{
  "name": "dude-cli",
  "version": "0.0.0",
  "description": "Scaffold regular copy paste things like a ninja",
  "bin": {
    "dude": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "start": "karma start karma.config.js",
    "test": "karma start karma.config.js --single-run",
    "lint": "tslint \"src/**/*.ts\"",
    "preversion": "yarn test",
    "version": "yarn build",
    "postversion": "git push origin && git push origin --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/harunurhan/dude.git"
  },
  "keywords": [
    "scaffolding",
    "typescript",
    "javascript",
    "generate",
    "create"
  ],
  "author": "@harunurhan <harunurhan17@gmail.com> (https://github.com/harunurhan)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/harunurhan/dude/issues"
  },
  "homepage": "https://github.com/harunurhan/dude#readme",
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/jasmine": "^2.5.54",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.9.4",
    "jasmine-core": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-typescript": "^3.0.5",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "@angular-devkit/core": "^0.8.1",
    "@types/ejs": "^2.6.0",
    "commander": "^2.17.1",
    "ejs": "^2.6.1",
    "mkdirp": "^0.5.1"
  }
}
